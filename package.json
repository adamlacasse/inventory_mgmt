{
  "name": "inventory_mgmt",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "corepack pnpm --filter @inventory/web dev",
    "build": "corepack pnpm -r --if-present build",
    "lint": "corepack pnpm -r --if-present lint",
    "typecheck": "corepack pnpm -r --if-present typecheck",
    "test": "corepack pnpm -r --if-present test",
    "test:e2e": "corepack pnpm --filter @inventory/web test:e2e",
    "db:migrate:status": "corepack pnpm --filter @inventory/db db:migrate:status",
    "check": "corepack pnpm lint && corepack pnpm typecheck && corepack pnpm test && corepack pnpm test:e2e && corepack pnpm db:migrate:status"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.13.1",
    "typescript": "5.7.3",
    "vitest": "3.0.5"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
